my_list = [1, 2, 3, 4, 5]
my_dict = {'a': 1, 'b': 2, 'c': 3}
my_set = {1, 2, 3, 4, 5}

# List operations
# Adding elements
my_list.append(6)
my_list.insert(0, 0)    
print("List after adding:", my_list)

# Removing elements
my_list.remove(3)           
popped = my_list.pop()      
print("List after removing:", my_list)
print("Popped element:", popped)

# Modifying elements
my_list[1] = 100        
print("List after modifying:", my_list)

# Dictionary operations
# Adding elements
my_dict['d'] = 4            
print("Dictionary after adding:", my_dict)

# Removing elements
del my_dict['b']            
removed = my_dict.pop('c')  
print("Dictionary after removing:", my_dict)
print("Removed value:", removed)

# Modifying elements
my_dict['a'] = 100          
print("Dictionary after modifying:", my_dict)

# Set operations
# Adding elements
my_set.add(6)               
my_set.update([7, 8, 9])    
print("Set after adding:", my_set)

# Removing elements
my_set.remove(2)        
my_set.discard(10)          
print("Set after removing:", my_set)

# Modifying elements
# Sets don't support direct modification, remove and add instead
my_set.remove(4)
my_set.add(40)
print("Set after modifying:", my_set)
